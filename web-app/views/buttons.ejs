<% include header %>
<h1>Buttons</h1>

<h2>Normal Buttons</h2>
<div>
    <button class="button">
        <span class="b-label">A Button</span>
    </button>
    <button class="button" disabled>
        <span class="b-label">A Disabled Button</span>
    </button>
</div>
<pre class="code prettyprint">
&lt;button class="button"&gt;
    &lt;span class="b-label"&gt;A Button&lt;/span&gt;
&lt;/button&gt;
&lt;button class="button" disabled&gt;
    &lt;span class="b-label"&gt;A Disabled Button&lt;/span&gt;
&lt;/button&gt;
</pre>

<h2>Action Buttons</h2>
<div>
